{
  "name": "@c1gee/n8n-nodes-companies-house",
  "version": "1.0.0",
  "description": "n8n community node for Companies House UK - Enhanced version based on original work by Ian Carroll",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/c1gee/n8n-nodes-companies-house#readme",
  "author": {
    "name": "Chris Gee",
    "email": "chris@withchrisgee.com"
  },
  "contributors": [
    {
      "name": "Ian Carroll",
      "email": "ian@solutioneers.co.uk"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/c1gee/n8n-nodes-companies-house.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CompaniesHouseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CompaniesHouse/CompaniesHouse.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "volta": {
    "node": "18.18.0"
  }
}
